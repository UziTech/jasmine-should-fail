{
  "name": "jasmine-should-fail",
  "version": "1.0.2",
  "description": "Tests will fail if they pass and pass if they fail",
  "repository": "https://github.com/UziTech/jasmine-should-fail",
  "main": "src/jasmine-should-fail.js",
  "scripts": {
    "test": "jasmine ./spec/*-spec.js",
    "release": "test-tag-publish",
    "release:patch": "test-tag-publish patch",
    "release:minor": "test-tag-publish minor",
    "release:major": "test-tag-publish major"
  },
  "keywords": [
    "jasmine",
    "fail"
  ],
  "author": "Tony Brix",
  "license": "MIT",
  "devDependencies": {
    "jasmine": "^2.5.3",
    "test-tag-publish": "^1.0.0"
  },
  "peerDependencies": {
    "jasmine": "2.x"
  }
}
